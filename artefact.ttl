	rdfs:range mbo:RoyaltyStatementType;
	rdfs:label "has royalty statement type"@en ;
	rdfs:comment "links a royalty statement with its type."@en .

mbo:hasSupplierId a rdf:Property;
	rdfs:domain mbo:RoyaltyStatement;
